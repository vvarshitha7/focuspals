{
  "manifest_version": 3,
  "name": "FocusPals",
  "version": "1.0",
  "description": "Track focused time, custom new tab, and virtual pet motivator.",
  "permissions": [
    "storage",
    "tabs",
    "alarms",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
